{
    "expo": {
        "sdkVersion": "31.0.0",
        "ios": {
            "bundleIdentifier": "com.polarduck.artisight",
            "supportsTablet": true,
            "config": {
                "usesNonExemptEncryption": false
            }
        },
        "android": {
            "package": "com.polarduck.artisight",
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/logo_android.png",
                "backgroundColor": "#150f19"
            }
        },
        "icon": "./assets/logo.png",
        "slug": "artisight",
        "privacy": "public",
        "splash": {
            "image": "./assets/splash.png",
            "backgroundColor": "#150f19"
        },
        "orientation": "portrait",
        "primaryColor": "#150f19",
        "packagerOpts": {
            "assetExts": ["ttf"]
        },
        "androidStatusBar": {
            "barStyle": "light-content",
            "backgroundColor": "#150f19"
        },
        "name": "Artisight",
        "version": "1.0.0",
        "description": "Valve Artifact tool for viewing cards and building decks"
    }
}
